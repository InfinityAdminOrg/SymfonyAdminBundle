{
    "name": "infinity-admin/symfony-admin-bundle",
    "description": "An experimental Symfony admin bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "pkly"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-json": "*",
        "symfony/dependency-injection": "^7",
        "symfony/form": "^7",
        "symfony/framework-bundle": "^7",
        "symfony/security-bundle": "^7",
        "symfony/serializer": "^7",
        "symfony/validator": "^7",
        "symfony/property-access": "^7",
        "symfony/property-info": "^7",
        "symfony/translation": "^7",
        "symfony/asset": "^7",
        "symfony/uid": "^7",
        "symfony/twig-bundle": "7.0.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1",
        "phpunit/phpunit": "^11",
        "friendsofphp/php-cs-fixer": "^3",
        "pedrotroller/php-cs-custom-fixer": "^2",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Infinity\\": "src"
        }
    }
